@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <h2>Les meilleurs restaurants de Grenoble</h2>
    <ul>
        @foreach (var p in ViewBag.allResto)
        {
            <li>
                @p.Name : @p.Commentary
            </li>
        }
    </ul>
</div>
